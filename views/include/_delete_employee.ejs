<div class="modal" id="deleteEmployeeModal">
    <div class="delete_modal" id="deleteEmployeeForm">
        <div class="row">
            <div class="col-11">
                <h4>Delete Employee</h4>
            </div>
            <div class="col-1">
                <button type="button" class="modal_btn" id="closeDeleteButton" onclick="closeDelEmployee()">
                    <span class="material-symbols-outlined">
                        close
                    </span>
                </button>
            </div>
        </div>

        <div class="row">
            <p>Are you sure you want to delete this employee?</p>
        </div>

        <div class="row">
            <div class="col delete_buttons" id="deletediv">
                <button type="button" class="butn cancel_btn" id="cancelDeleteButton"
                    onclick="closeDelEmployee()">
                    Cancel
                </button>
                <button type="button" class="butn delete_btn" id="deleteButton">
                    Delete
                </button>
            </div>
        </div>
    </div>
</div>